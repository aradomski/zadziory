<%= semantic_form_for @history do |f| %>
  <%= f.inputs do %>
    <%= f.input :date_start %>
    <%= f.input :date_end %>
    <%= f.input :comment_item %>
    <%= f.input :comment_tenant %>
    <%= f.input :comment_owner %>
    <%= f.input :rate_item %>
    <%= f.input :rate_tenant %>
    <%= f.input :rate_owner %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
