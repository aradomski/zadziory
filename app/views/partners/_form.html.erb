<%= semantic_form_for @partner do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :surname %>
    <%= f.input :yob %>
    <%= f.input :anniversary %>
    <%= f.input :flower %>
    <%= f.input :color %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
