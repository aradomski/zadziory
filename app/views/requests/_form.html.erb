<%= semantic_form_for @request do |f| %>
  <%= f.inputs do %>
    <%= f.input :user_id %>
    <%= f.input :place_id %>
    <%= f.input :owner_id %>
    <%= f.input :status, :as => :enum_select %>
    <%= f.input :date_start %>
    <%= f.input :date_end %>
    <%= f.input :number_of_people %>
    <%= f.input :people_type %>
    <%= f.input :message %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
