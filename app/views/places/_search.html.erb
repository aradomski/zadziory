<%= semantic_form_for @search do |f| %>
	<%= f.inputs do %>

    <%= f.input :city_contains, :input_html => { :size => 20 } %>

	<%= f.input :price_min_gte, :label => "Cena od", :input_html => { :size => 5 } %>
	<%= f.input :price_min_lte, :label => "Cena do", :input_html => { :size => 5 } %>
	
	<% end %>
	
	<fieldset class="knob">
	<%= f.buttons do %>
		<%= f.commit_button :label => "Szukaj" %>
	<% end %>
	</fieldset>
	
<% end %>
